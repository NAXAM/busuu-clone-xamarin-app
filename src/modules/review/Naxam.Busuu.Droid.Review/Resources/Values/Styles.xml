<resources>
  <style name="AppTheme.Premium" parent="Theme.AppCompat.Light.DarkActionBar">
    <!-- Customize your theme here. -->
    <item name="colorPrimary">#FEAB35</item>
    <item name="colorPrimaryDark">#D38E2C</item>
    <item name="colorAccent">@color/colorAccent</item>
    <item name="android:textColorSecondary">@color/colorHintEditText</item>
  </style>
  <!-- Base application theme. -->
  <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
    <item name="colorPrimary">@color/colorPrimary</item>
    <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
  </style>
  <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
    <!-- Customize your theme here. -->
    <item name="colorPrimary">@color/colorPrimary</item>
    <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    <item name="colorAccent">@color/colorAccent</item>
    <item name="android:textColorSecondary">@color/colorHintEditText</item>
  </style>
  <style name="NoActionBarTheme" parent="Theme.AppCompat.Light.NoActionBar">
    <!-- Customize your theme here. -->
    <item name="colorPrimary">@color/colorPrimary</item>
    <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    <item name="colorAccent">@color/colorAccent</item>
    <item name="android:textColorSecondary">@color/colorHintEditText</item>
  </style>
  <!--Style Text Title-->
  <style name="StyleTextFloatMenu">
    <item name="android:textSize">14sp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:textColor">#010101</item>
    <item name="android:textAllCaps">true</item>
  </style>
  <style name="styleTextDescription">
    <item name="android:textColor">#A7B0B7</item>
    <item name="android:textSize">14sp</item>
  </style>
  <style name="styleTextNormal">
    <item name="android:textColor">#3E3E3E</item>
    <item name="android:textSize">16sp</item>
  </style>
  <style name="styleTextHeader">
    <item name="android:textColor">#252B2F</item>
    <item name="android:textSize">16sp</item>
    <item name="android:textStyle">bold</item>
  </style>
  <style name="StyleTextTitle">
    <item name="android:textColor">@color/colorTextPrimary</item>
    <item name="android:textSize">@dimen/textSizeTitle</item>
  </style>

  <style name="StyleTextTitleRegular" parent="@style/StyleTextTitle"></style>

  <style name="StyleTextTitleBold" parent="@style/StyleTextTitle">
    <item name="android:textStyle">bold</item>
  </style>
  <!--Style Text Subtitle-->
  <style name="StyleTextSubTitle">
    <item name="android:textColor">@color/colorTextSecondary</item>
    <item name="android:textSize">@dimen/textSizeSubTitle</item>
  </style>
  <!--Style Button-->
  <style name="StyleButton">
    <item name="android:layout_height">@dimen/buttonHeightDefault</item>
  </style>

  <style name="StyleButtonConfirmDialog" parent="StyleButton">
    <item name="android:textColor">@color/colorPrimary</item>
    <item name="android:background">@android:color/transparent</item>
  </style>
  <!--Style EditText-->
  <style name="StyleEditText">
    <item name="android:textColorHint">@color/colorHintEditText</item>
    <item name="android:textColorHighlight">@color/colorPrimary</item>
    <item name="android:textColor">@color/colorTextPrimary</item>
    <item name="android:textSize">@dimen/textSizeSubTitle</item>
    <item name="android:layout_height">@dimen/buttonHeightDefault</item>
  </style>
  <!--Style ProgressBar-->
   
  
</resources>
